#include <stdio.h>

int main(void)
{
    int T, a, b;
    scanf("%d", &T);
    
    for(int i=0;i<T;i++)
    {
        scanf("%d %d", &a, &b);
        int r=a;
        for(int j=b; j>1; j--)
        {
            a*=r;
        }
        if(a%10 == 0)
            printf("10\n");
        else{
            printf("%d\n", a%10);
            printf("%d\n", a);
        }
    }
    
return 0;
}
